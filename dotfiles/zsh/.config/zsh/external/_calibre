_ebook_edit() {
    local curcontext="$curcontext" state line ebookfile expl
    typeset -A opt_args

    _arguments -C -s \
        "--version""[affiche le numéro de version du logiciel et quitte]" \
        {--help,-h}"[affiche ce message d'aide et quitte]" \
        "--detach""[Détacher du terminal de contrôle, s'il y en a un (linux uniquement)]" \
        "--select-text=""[Le texte à sélectionner dans le livre lorsqu'il est ouvert pour être édité]" \
        "1:ebook file:_files -g '(#i)*.(EPUB|AZW3|xhtml|htm|docx|html|xhtm)'" \
        '*:file in ebook:->files' && return 0

    case $state in
        files)
            ebookfile=${~${(Q)line[1]}}

            if [[ -f "$ebookfile" && "$ebookfile" =~ '\.[eE][pP][uU][bB]$' ]]; then
                _zip_cache_name="$ebookfile"
                _zip_cache_list=( ${(f)"$(zipinfo -1 $_zip_cache_name 2>/dev/null)"} )
            else
                return 1
            fi
            _wanted files expl 'file from ebook' \
            _multi_parts / _zip_cache_list && return 0
            ;;
    esac

    return 1
}


case $service in
calibre)
_arguments -s \
  '(- *)'--version"[affiche le numéro de version du logiciel et quitte]" \
  '(- *)'{--help,-h}"[affiche ce message d'aide et quitte]" \
  --with-library="[Utiliser la bibliothèque présente dans le dossier spécifié.]":"Utiliser la bibliothèque présente dans le dossier spécifié.":'_path_files -/' \
  --start-in-tray"[Démarrer en mode minimisé dans la zone de notification.]" \
  {--verbose,-v}"[Ignoré, ne pas utiliser. Présent uniquement pour d'anciennes raisons]" \
  --no-update-check"[Ne pas vérifier les mises à jour]" \
  --ignore-plugins"[Ignorer les extensions personnalisées, utile si vous installez une extension qui empêche calibre de démarrer.]" \
  {--shutdown-running-calibre,-s}"[Provoque la fermeture de la session de calibre en cours (si elle existe). Remarquez que s’il y a des tâches en cours, elles seront annulées sur le champ. À utiliser avec prudence.]" \
  --detach"[Détacher du terminal de contrôle, s'il y en a un (linux uniquement)]" \
  "*:filename:_files -g '(#i)*.(azw|azw1|azw3|azw4|book|cb7|cbc|cbr|cbz|chm|djv|djvu|doc|docm|docx|epub|fb2|fbz|htm|html|htmlz|iba|ibook|ibooks|imp|kepub|kfx|kpf|lit|lrf|lrx|markdown|mbp|md|mobi|odt|oebzip|oxps|pdb|pdf|pdr|pml|pmlz|pobi|prc|ps|rar|rb|rtf|snb|tan|text|textile|tpz|txt|txtz|updb|xhtm|xhtml|xps|zbf|zip)'" 
;;
lrf2lrs)
_arguments -s \
  '(- *)'--version"[affiche le numéro de version du logiciel et quitte]" \
  '(- *)'{--help,-h}"[affiche ce message d'aide et quitte]" \
  {--output=,-o+}"[Fichier de sortie LRS]":"Fichier de sortie LRS":'_files -g "*.lrs *.LRS"' \
  --dont-output-resources"[Ne pas enregistrer les images incorporées et les fichiers de polices sur le disque]" \
  --verbose"[Soyez plus détaillé]" \
  "*:filename:_files -g '(#i)*.(lrf)'" 
;;
